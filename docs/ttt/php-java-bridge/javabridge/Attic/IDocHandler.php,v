  */
class javabridge_ParserString {
  var $string, $off, $length;
  function toString() {
	return $this->getString();
  }
  function getString() {
	return substr($this->string, $this->off, $this->length);
  }
  function getExact() {
	return hexdec($this->getString());
  }
  function getInexact() {
	sscanf($this->getString(), "%e", $val);
	return $val;
  }
}
class javabridge_ParserTag {
  var $n, $strings;
  function javabridge_ParserTag() {
	$this->strings = array();
	$this->n = 0;
  }
}
?>
